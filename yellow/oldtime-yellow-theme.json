  "tokenColors": [
    {
      "scope": ["comment"],
      "settings": {
        "foreground": "#aaaa00",
        "fontStyle": "italic"
      }
    },
    {
      "scope": ["string"],
      "settings": {
        "foreground": "#00ff00"
      }
    },
    {
      "scope": ["keyword", "storage.type", "storage.modifier"],
      "settings": {
        "foreground": "#ff00ff",
        "fontStyle": "bold"
      }
    },
    {
      "scope": ["entity.name.function", "support.function"],
      "settings": {
        "foreground": "#ffff00"
      }
    },
    {
      "scope": ["variable", "entity.name.variable"],
      "settings": {
        "foreground": "#ffff00"
      }
    },
    {
      "scope": ["constant.numeric", "constant.language"],
      "settings": {
        "foreground": "#ff00ff"
      }
    },
    {
      "scope": ["entity.name.type", "entity.name.class"],
      "settings": {
        "foreground": "#00ffff"
      }
    },
    {
      "scope": ["support.type", "support.class"],
      "settings": {
        "foreground": "#00ffff"
      }
    },
    {
      "scope": ["punctuation"],
      "settings": {
        "foreground": "#ffff00"
      }
    },
    {
      "scope": ["markup.heading"],
      "settings": {
        "foreground": "#00ffff",
        "fontStyle": "bold"
      }
    },
    {
      "scope": ["markup.bold"],
      "settings": {
        "foreground": "#00ff00",
        "fontStyle": "bold"
      }
    },
    {
      "scope": ["markup.italic"],
      "settings": {
        "foreground": "#ff00ff",
        "fontStyle": "italic"
      }
    },
    {
      "scope": ["markup.underline.link", "markup.underline.link.markdown"],
      "settings": {
        "foreground": "#00ffff",
        "fontStyle": "underline"
      }
    },
    {
      "scope": [
        "string.other.link.title.markdown",
        "string.other.link.description.markdown"
      ],
      "settings": {
        "foreground": "#00ff00"
      }
    },
    {
      "scope": ["markup.inline.raw", "markup.inline.raw.string.markdown"],
      "settings": {
        "foreground": "#ff00ff"
      }
    },
    {
      "scope": [
        "markup.fenced_code.block.markdown",
        "markup.raw.block.markdown"
      ],
      "settings": {
        "foreground": "#ffff00"
      }
    },
    {
      "scope": ["markup.quote", "markup.quote.markdown"],
      "settings": {
        "foreground": "#aaaa00",
        "fontStyle": "italic"
      }
    },
    {
      "scope": [
        "markup.list",
        "markup.list.unnumbered.markdown",
        "markup.list.numbered.markdown"
      ],
      "settings": {
        "foreground": "#ffff00"
      }
    },
    {
      "scope": [
        "punctuation.definition.heading.markdown",
        "punctuation.definition.list.begin.markdown",
        "punctuation.definition.quote.begin.markdown",
        "punctuation.definition.raw.markdown",
        "punctuation.definition.markdown"
      ],
      "settings": {
        "foreground": "#aaaa00"
      }
    },
    {
      "scope": [
        "punctuation.definition.string.begin.markdown",
        "punctuation.definition.string.end.markdown",
        "punctuation.definition.metadata.markdown"
      ],
      "settings": {
        "foreground": "#aaaa00"
      }
    },
    {
      "scope": ["markup.strikethrough"],
      "settings": {
        "foreground": "#808080",
        "fontStyle": "strikethrough"
      }
    },
    {
      "scope": ["beginning.punctuation.definition.list.markdown"],
      "settings": {
        "foreground": "#cccc00"
      }
    },
    {
      "scope": ["meta.separator.markdown"],
      "settings": {
        "foreground": "#aaaa00"
      }
    },
    {
      "scope": ["entity.name.tag"],
      "settings": {
        "foreground": "#00ffff"
      }
    },
    {
      "scope": ["entity.other.attribute-name"],
      "settings": {
        "foreground": "#00ff00"
      }
    },
    {
      "scope": ["support.constant", "constant.other"],
      "settings": {
        "foreground": "#ff00ff"
      }
    },
    {
      "scope": ["meta.function-call", "entity.name.function.call"],
      "settings": {
        "foreground": "#ffff00"
      }
    },
    {
      "scope": ["keyword.operator"],
      "settings": {
        "foreground": "#ff00ff"
      }
    },
    {
      "scope": ["variable.parameter", "meta.parameter"],
      "settings": {
        "foreground": "#00ff00"
      }
    }
  ]
}
